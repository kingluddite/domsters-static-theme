{
  "name": "domsters-static-theme",
  "eslintConfig": {
    "extends": [
      "plugin:wordpress/recommended",
      "plugin:wordpress/node"
    ],
    "plugins": [
      "wordpress"
    ]
  },
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:kingluddite/domsters-static-theme.git",
  "browserslist": [
    "> 1%",
    "ie > 9"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "author": "Kingluddite <howley.phil@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "webpack --watch",
    "build": "NODE_ENV=production npm run clean && webpack --mode production",
    "lint": "eslint --fix src/*.js",
    "pretest": "npm run lint",
    "precommit": "lint-staged",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "dev-server": "webpack-dev-server --mode development",
    "production": "yarn run clean && cross-env NODE_ENV=production webpack --mode production",
    "clean": "rimraf ./dist/*"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "autoprefixer": "^8.2.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "browser-sync": "^2.23.6",
    "browser-sync-webpack-plugin": "^2.2.2",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "dotenv": "^5.0.1",
    "eslint": "^4.19.1",
    "eslint-plugin-wordpress": "^0.1.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "image-webpack-loader": "^4.2.0",
    "node-sass": "^4.8.3",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "postcss-loader": "^2.1.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.20.3",
    "uglify-js": "^3.3.16",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-loader": "^1.0.1",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.5",
    "@fortawesome/fontawesome-free-brands": "^5.0.10",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "faker": "^4.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "lodash": "^4.17.5",
    "normalize.css": "^8.0.0",
    "prettier": "^1.12.1",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.1",
    "raw-loader": "^0.5.1"
  }
}
